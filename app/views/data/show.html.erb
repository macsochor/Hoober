<div class="page-header">
  <%= link_to data_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_datum_path(@datum), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show datum</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @datum.user.name %></dd>

  <dt>Csv:</dt>
  <dd><%= link_to "CSV", attachment_url(@datum, :csv) %></dd>

</dl>
